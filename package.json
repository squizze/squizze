{
  "name": "DISC",
  "version": "1.0.0",
  "description": "Teste de perfil comportamental DISC.",
  "main": "src/js/index.js",
  "scripts": {
    "test": "karma start --browsers PhantomJS",
    "coverage": "cat coverage/lcov/lcov.info | ./node_modules/codacy-coverage/bin/codacy-coverage.js",
    "build": "browserify ./src/js/index.js -o bundle.js",
    "watch": "watchify ./src/js/index.js -o bundle.js",
    "template-cache": "node node_modules/angular-template-cache/bin/cmd.js -f './src/**/*.template.html' -o ./src/js/templates/templates.run.js -s browserify --no-new-module -m disc.templates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teles/DISC.git"
  },
  "keywords": [
    "test",
    "disc",
    "portugues",
    "brasil",
    "testes"
  ],
  "author": "Teles <josetelesmaciel@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teles/DISC/issues"
  },
  "homepage": "https://github.com/teles/DISCc#readme",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.3",
    "angular": "^1.6.5"
  },
  "devDependencies": {
    "angular-mocks": "1.6.5",
    "angular-template-cache": "1.2.0",
    "browserify": "^14.4.0",
    "codacy-coverage": "2.0.2",
    "common.js": "1.1.1",
    "connect": "3.6.2",
    "eslint": "4.0.0-rc.0",
    "jasmine": "2.7.0",
    "karma": "1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "0.1.0",
    "karma-phantomjs-launcher": "1.0.4",
    "serve-static": "1.12.3",
    "watchify": "^3.9.0"
  }
}
